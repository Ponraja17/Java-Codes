import java.util.Scanner;
import java.util.*;
public class Dublicat_Array {
public static void main(String args[]) {
	int i,j,l=0;
	int []a= {1,2,5,5,6,7,6,2};
	int c[]=new int[4];
	for(i=0;i<a.length;i++) {
		for(j=i+1;j<a.length;j++) {
			if(a[i]==a[j]) {
				
				c[l]=a[i];
				l++;
			}
		}
	}
	System.out.print(Arrays.toString(c));
}
}
